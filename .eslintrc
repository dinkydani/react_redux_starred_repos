{
  "extends": ["airbnb/base"],
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "arrow-body-style": 0,
    "comma-dangle": [2, "never"],
    "no-console": 0,
    "func-names": 0,
    "semi": [0, "never"],
    "react/jsx-uses-vars": 1,
    "react/jsx-uses-react": 2
  }
}